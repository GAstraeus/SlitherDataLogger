{ 
"manifest_version" : 2, 
"name" : "Slither Scraper", 
"version" : "0.1", 
"web_accessible_resources" : [ 
"images/*.jpg"],
 "background" : { 
"scripts" : ["background.js"]
 }, 
 "content_scripts" : [
{ 
"matches" : [ 
"<all_urls>"
], 
"js" : ["content.js"]
}
],
 "browser_action": {
      "default_popup": "popup.html"
      
    },

    "permissions": [
        "tabs",
        "activeTab",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}